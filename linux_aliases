# The next line updates PATH for the Google Cloud SDK.
if [ -f '/usr/share/google-cloud-sdk/path.zsh.inc' ]; then source '/usr/share/google-cloud-sdk/path.zsh.inc'; fi
if [ -f '/usr/share/google-cloud-sdk/completion.zsh.inc' ]; then source '/usr/share/google-cloud-sdk/completion.zsh.inc'; fi

alias vscode=" code-server -H -N -e /root/.vsextentions"