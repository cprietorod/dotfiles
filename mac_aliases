export PATH=$PWD/:$PATH:$HOME/Desarrollo/scripts:$HOME/Desarrollo/google-cloud-sdk/platform/google_appengine:$HOME/Desarrollo/google-cloud-sdk/bin
export GCLOUD1='docker run --rm -p 3000:3000 -p 8443:8443  -p 8000:8000 -v /var/run/docker.sock:/var/run/docker.sock -v ~/:/root:cached -v ~/.config:/root/.config:cached  -v /Users:/Users:cached '
export GCLOUD2='-v $PWD:/var/gcloud:cached -w /var/gcloud'
export GCLOUD3='cprietorod/my-cloud-sdk-docker:latest'
export GCLOUD_COMMAND="$GCLOUD1 -ti $GCLOUD2 $GCLOUD3"
alias whatismyip="curl -s checkip.dyndns.org | sed -e 's/.*Current IP Address: //' -e 's/<.*$//'"
alias mysqld_get_ip="docker inspect -f '{{range .NetworkSettings.Networks}}{{.IPAddress}}{{end}}' dev_mysql_1"
alias mysql="docker ps | grep  dev_mysql_1 || docker start  dev_mysql_1 ; docker exec -it dev_mysql_1  mysql"
alias dev_bash='docker ps | grep  dev_env_1 || docker start  dev_env_1 ; docker exec -it -w $PWD dev_env_1 bash'
alias dev_zsh='docker ps | grep  dev_env_1 || docker start  dev_env_1 ; docker exec -it -w $PWD dev_env_1 zsh'
alias vscode="docker stop dev_env_1 || echo 'no hay docker' ; docker start dev_env_1; docker exec -d dev_env_1  code-server --host 0.0.0.0 --port 8443 --extra-extensions-dir /root/.vsextentions  --auth none --user-data-dir /root/.vscode/ ;  open -a Coder "
alias vscode_only_chrome='open -a Coder'
