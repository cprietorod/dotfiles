	"scripts": {
		"copy-clipboardy-fallbacks": "copyfiles -u 2 \"./node_modules/clipboardy/fallbacks/**\" ./",
		"copy-markdowns": "copyfiles -u 1 \"cloud-code-vscode/*.md\" ./",
		"vscode:prepublish": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --mode production --devtool none && cd ./src/webview && npm run build && cd ../../third_party/yaml-language-server && npm run compile-prod && cd ../../ && npm run copy-clipboardy-fallbacks && npm run copy-markdowns",
		"compile": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --mode none && cd ./src/webview && npm run build && cd ../../third_party/yaml-language-server/ && npm run compile",
		"watch": "npm run compile && webpack --mode none --watch",
		"watch:kubernetes": "npm run compile && webpack --mode none --watch --info-verbosity verbose",
		"watch:kubernetes:test": "tsc -watch -p ./",
		"watch:yaml-language-server": "cd third_party/yaml-language-server && webpack --mode none --watch --info-verbosity verbose",
		"watch:cloud-debug": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install && cd ./src/webview && npm install && cd ../../third_party/yaml-language-server && npm install",
		"test": "cross-env CODE_TESTS_PATH=$INIT_CWD/dist/test node ./node_modules/vscode/bin/test && cd ./src/webview && npm run test && cd ../../third_party/yaml-language-server && npm run test",
		"check": "gts check && cd ./src/webview && npm run lint && cd ../../third_party/yaml-language-server/ && npm run check",
		"clean": "gts clean && cd ./third_party/yaml-language-server && npm run clean",
		"package": "vsce package",
		"fix": "gts fix && cd ./third_party/yaml-language-server && npm run fix",
		"test-compile": "tsc -p . && cd ./src/webview && npm run test-build && cd ../../third_party/yaml-language-server/ && npm run test-compile",
		"pretest": "npm run test-compile",
		"posttest": "npm run check"
	},
